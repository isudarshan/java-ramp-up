			<name>The Apache Software License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
		</license>
	</licenses>

	<organization>
		<name>Sudarshan</name>
		<url>https://github.com/isudarshan/</url>
	</organization>
	<developers>
		<developer>
			<id>Sudarshan</id>
			<name>Sudarshan</name>
			<email>isudarshanambatipudi@gmail.com</email>
			<organization>CodeZarvis</organization>
			<roles>
				<role>Scrum Master</role>
				<role>Engineer</role>
			</roles>
			<properties>
				<location>India</location>
			</properties>
			<url>N/A</url>
		</developer>
	</developers>

</project>
